Host skip
    HostName            skip.grnoc.iu.edu

    ForwardAgent        yes

    ControlMaster       auto
    ControlPath         ~/.ssh/sockets/%r@%h:%p
    ControlPersist      15m

    ProxyCommand        none

Host *
    ProxyCommand        ssh -W %h:%p skip

    ServerAliveCountMax 3
    ServerAliveInterval 30
