Host *
    Compression         yes

    ControlMaster       auto
    ControlPath         ~/.ssh/sockets/%r@%h:%p
    ControlPersist      15m

    ProxyCommand        ssh -W %h:%p -q skip

    ServerAliveCountMax 3
    ServerAliveInterval 30

Host skip leap jump laptop xaiver
    ProxyCommand        none

Host skip leap jump
    ForwardAgent        yes

Host jump
    HostName            jump.grnoc.iu.edu

Host laptop
    HostName            jdratlif-yoga2.uits.iu.edu
    User                john@technoplaza.net
    Port                9722
    RemoteForward       2222 localhost:22
    RemoteForward       5907 localhost:5901

Host leap
    HostName            leap.grnoc.iu.edu

Host skip
    HostName            skip.grnoc.iu.edu

Host vmx
    HostName            140.182.45.188
    User                root

Host xaiver
    HostName            xaiver.technoplaza.net
    Port                2202
    RemoteForward       192.168.1.2:5907 localhost:5901
    RemoteForward       192.168.1.2:9722 localhost:22
